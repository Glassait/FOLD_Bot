{
	"name": "fold_bot",
	"version": "3.3.1",
	"description": "A bot for the FOLD_ discord server and WoT clan",
	"main": "src/index.ts",
	"engines": {
		"node": ">= 18.18.0"
	},
	"scripts": {
		"start": "ts-node -r tsconfig-paths/register .",
		"start:mock": "ts-node -r tsconfig-paths/register . -- DEV",
		"build": "tsc --build",
		"clean": "tsc --build --clean",
		"lint": "eslint src",
		"prepare": "husky"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Glassait/FOLD_Bot.git"
	},
	"keywords": [
		"discord",
		"typescript",
		"discordjs",
		"bot"
	],
	"lint-staged": {
		"*": "prettier -w",
		"*.ts": "eslint"
	},
	"author": "Glassait",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Glassait/FOLD_Bot/issues"
	},
	"homepage": "https://github.com/Glassait/FOLD_Bot#readme",
	"madge": {
		"detectiveOptions": {
			"fontSize": "12px",
			"layout": "sfdp",
			"graphVizOptions": {
				"G": {
					"rankdir": "LR",
					"beautify": true
				}
			},
			"es6": {
				"mixedImports": true,
				"skipTypeImports": true
			}
		}
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.58",
		"axios": "^1.7.7",
		"cheerio": "1.0.0-rc.12",
		"cron": "^3.1.7",
		"discord.js": "^14.16.3",
		"mysql2": "^3.11.3",
		"tsconfig-paths": "^4.2.0",
		"ts-node": "10.9.2"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.12.0",
		"@types/node": "^20.16.11",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"eslint": "^8.57.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-promise": "^6.6.0",
		"globals": "^15.11.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"prettier": "3.2.5",
		"typescript": "^5.6.3",
		"typescript-eslint": "^7.18.0"
	}
}
